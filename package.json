{
  "name": "pg6301-innlevering",
  "version": "1.0.0",
  "description": "Innlevering av Christian Gregersen og Marie Stigen for Webutvikling og API-design",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "prettier": "prettier --write **/*.{js,jsx,ts,tsx}",
    "prettier:check": "prettier --check **/*.{js,jsx,ts,tsx}",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --collectCoverage",
    "verify": "npm run prettier:check && npm run test",
    "dev": "concurrently npm:dev:client npm:dev:server",
    "dev:server": "cd client && npm run dev",
    "dev:client": "cd server && npm run dev",
    "prepare": "husky install",
    "pre-commit": "npm run prettier:check",
    "build": "npm install && npm run build:server && npm run build:client",
    "build:server": "cd server && npm run build",
    "build:client": "cd client && npm run build",
    "start": "cd server && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kristiania-pg6301-2022/pg6301-innlevering-mariestigen.git"
  },
  "keywords": [
    "innlevering",
    "web",
    "api",
    "pg6301",
    "react",
    "express"
  ],
  "author": "Christian Gregersen & Marie Stigen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kristiania-pg6301-2022/pg6301-innlevering-mariestigen/issues"
  },
  "homepage": "https://github.com/kristiania-pg6301-2022/pg6301-innlevering-mariestigen#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-jest": "^27.5.1",
    "concurrently": "^7.0.0",
    "eslint": "^8.9.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "engines": {
    "node": "^17.4.0",
    "npm": "^8.5.1"
  }
}
